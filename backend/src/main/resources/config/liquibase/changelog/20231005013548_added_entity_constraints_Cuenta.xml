<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Cuenta.
    -->
    <changeSet id="20231005013548-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="cliente_id"
                                 baseTableName="cuenta"
                                 constraintName="fk_cuenta__cliente_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cliente"/>

        <addForeignKeyConstraint baseColumnNames="retiros_id"
                                 baseTableName="cuenta"
                                 constraintName="fk_cuenta__retiros_id"
                                 referencedColumnNames="id"
                                 referencedTableName="retiro"/>
    </changeSet>
</databaseChangeLog>
